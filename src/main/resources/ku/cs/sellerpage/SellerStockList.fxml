<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="55.0" prefWidth="915.0" spacing="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.seller.SellerStockList">
   <children>
      <ImageView fx:id="img" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/sellerpage/profile.png" />
         </image>
      </ImageView>
      <Label fx:id="nameProduct" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="154.0" text="ชื่อสินค้า">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="quantity" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="81.0" text="n">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="price" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="101.0" text="n">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="name" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="145.0" text="n">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="productStatus" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="122.0" text="n">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="parcelNumber" alignment="CENTER" contentDisplay="TOP" prefHeight="53.0" prefWidth="133.0" text="n">
         <font>
            <Font name="Kanit Regular" size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="68.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="44.0" prefWidth="60.0" style="-fx-background-radius: 10;" text="แก้ไข" />
         </children>
      </HBox>
   </children>
</HBox>
